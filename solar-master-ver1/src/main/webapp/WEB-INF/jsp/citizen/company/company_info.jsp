<!-- popup for company information -->
<div class="pop-wrap">
    <div class="pop-wrap-content">
        <button class="pop-close">close</button>
        <div class="pop-wrap-content-inner">
            <div class="inner-title">
                <img src="<%=contextPath%>img/c-logo1.png" alt="">
                <div class="tBox">
                    <h2></h2>
                    <p id="contact"></p>
                    <p id="address"></p>
                </div>
            </div>
            <div class="inner-item">
                <div class="exp-box">
                    <p id="exp"></p>
                </div>
                <input type="button" value="설비 제품 확인">
            </div>
        </div>
    </div>
</div>
<!-- end popup for company information -->
<script>
$(document).ready(function () {
    $('.company-content-wrap-grid div').click(function(){
        var id = $(this).children('img').attr('src').split('.')[0].split('logo')[1];
        var name = $(this).children('h2').text();
        var url = "${pageContext.request.contextPath}/citizen/company-information/" + id + ".do";
        $.ajax({
            type: 'GET',
            url: url,
            cache: false,
            success: function(company) {
                $('.pop-wrap-content-inner div img').attr('src','<%=contextPath%>/img/c-logo'+id+'.png');
                $('.pop-wrap-content-inner h2').text(name);
                $('.pop-wrap-content-inner p#contact').html(company.contact || "&#160;");
                $('.pop-wrap-content-inner p#address').html(company.address || "&#160;");
                $('.pop-wrap-content-inner p#exp').html(company.information || "2019년 베란다형 태양광 미니발전소 보급업체입니다.");
                //$('.pop-wrap-content-inner > div .tBox p').text(companyPhonnumber[num]);
                $('.pop-wrap-content-inner > div input').attr("onclick","location.href='<%=request.getContextPath()%>/citizen/product/info.do?name="+encodeURIComponent(name)+"'");
                setTimeout(function(){ $('.pop-wrap').addClass('pop-open'); }, 100);
                // $('.pop-wrap').addClass('pop-open');
            },
            error: function(xhr, status, error) {
                console.log(eval("(" + xhr.responseText + ")"));
            }
        });
    });
});
</script>




















